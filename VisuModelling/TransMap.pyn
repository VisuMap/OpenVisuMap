# VsTrain.pyn
vv.Import('VsModelling.pyn')

if 'vmd' not in globals():
	vmd = ModellingConfiguration()

vmd.Clear()
vmd.ds = TrainDataset(GetMapData(), GetMapData('2D'))
inDim, outDim = vmd.ds.X.shape[1], vmd.ds.Y.shape[1]
dimList, DropR, LR  = [inDim, 16, 32, 64, 32, 16, outDim], 0.5, 0.0001

vmd.model = NewMapModel(dimList, vmd.ds.Y, DropR)
vmd.EnableLog()
TrainModel(vmd, epochs = 2000, initial_lr=LR)
ShowPred(vmd.model(vmd.ds.X, training=False).numpy(), vmd.ds.Y)
