def WaveSizeTable(P, pSeq, M=50):
	L = len(P)
	szMtr = np.zeros((L, M), np.int32)
	posV = np.array(L*[-1])
	for k, c in enumerate(pSeq):
		row = P[c]
		sz = k - posV[row]
		if sz < M:
			szMtr[row, sz] += 1
		posV[row] = k
	return szMtr.reshape(-1).astype(np.float32)

