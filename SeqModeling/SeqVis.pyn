def CreateProteinMap(idList, epochs=10000, is3D=False, initExag=10.0, 
		finalExag=1.0, pp=0.1, decay=20.0, repeats=1, saveTo=None):
	pTable = GetProteinTable()
	P = {a:k for k,a in enumerate(ALPHABET)}
	L, M, N = len(P), 50, len(idList)
	nt = np.zeros((N, L), np.float32)
	ww = np.array([1.0/(k+decay) for k in range(M)]).reshape((-1, 1))
	#ww =  np.array(list(range(M))).reshape((-1, 1))
	#ww = np.log(1.0+weight)
	#ww = np.sqrt(weight)
	# aaSize holds the period size, up to M, for each amino-acid in pSeq.
	# aaPos[] holds the last sequencial-position of each amino-acid.
	aaSize = np.empty((L, M), np.int32)
	aaPos = np.empty((L), np.int32)
	for n, pId in enumerate(idList):
		pSeq = GetProteinSeq(pTable, pId)
		aaSize[:,:] = 0
		aaPos[:] = -1
		for k, c in enumerate(pSeq):
			aaIdx = P[c]
			sz = min(M, k - aaPos[aaIdx])			
			aaSize[aaIdx, sz-1] += 1  # 0-column count number of period of size 1.
			aaPos[aaIdx] = k
		nt[n] = np.matmul(aaSize, ww).reshape(-1)
	dt = mm.ToTable(nt)
	for k, b in enumerate(vv.Dataset.BodyListForId(idList)):
		dt.RowSpecList[k].CopyFromBody(b)
	mds = NewMds(dt, epochs=epochs, is3D=is3D, initExag=initExag, finalExag=finalExag, pp=pp)
	RunMds(mds, repeats=repeats, glyphSize=0.25, glyphOpacity=0.5)
	if saveTo is not None:
		SaveToAtlas(saveTo)
		mds.Close()

