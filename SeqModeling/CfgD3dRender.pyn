#MenuLabels ShowData Scanning ShowSeq ReCreateMap Smoothen UnFold

vv.Import('SeqVis.pyn')

match vv.EventSource.Item:
	case 'ShowData':
		AssertSeqMap(pp)
		ShowMapData(pp)
	case 'Scanning':		
		Scanning(pp, stepSize=1)
	case 'ShowSeq':
		AssertSeqMap(pp)
		ShowAASeq(pp)
	case 'ReCreateMap':
		AssertSeqMap(pp)
		ReCreateMap(pp)
	case 'Smoothen':
		SmoothenCurve(pp, repeats=10)
	case 'UnFold':
		UnFoldMap(pp, epochs=2000, repeats=1, is3D=False, finalExag=1.0, ppl=0.15, stretch=0.002)
