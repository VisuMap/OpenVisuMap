#MenuLabels ShowData Scanning ShowSeq ReCreateMap Smoothen To2D

vv.Import('SeqVis.pyn')

match vv.EventSource.Item:
	case 'ShowData':
		AssertSeqMap(pp)
		ShowMapData(pp)
	case 'Scanning':		
		Scanning(pp, stepSize=20)
	case 'ShowSeq':
		AssertSeqMap(pp)
		ShowAASeq(pp)
	case 'ReCreateMap':
		AssertSeqMap(pp)
		ReCreateMap(pp)
	case 'Smoothen':
		SmoothenCurve(pp, repeats=1)
	case 'To2D':
		To2DMap(pp, repeats=1, ppl=0.15, typeExpand=0)
